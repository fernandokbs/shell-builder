{
    "name": "fernandokbs/shell-builder",
    "description": "This is my package shell-builder",
    "keywords": [
        "fernandokbs",
        "shell-builder"
    ],
    "homepage": "https://github.com/fernandokbs/shell-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando",
            "email": "fernando93d@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/process": "^4.4|^5.3|^6.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Fernandokbs\\ShellBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fernandokbs\\ShellBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
